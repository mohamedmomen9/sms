{
    "name": "nwidart/payment-module",
    "type": "laravel-module",
    "description": "A reusable Laravel module: Payment",
    "keywords": [
        "laravel",
        "module",
        "payment"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Payment\\": "app/",
            "Modules\\Payment\\Database\\Factories\\": "database/factories/",
            "Modules\\Payment\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Payment\\Providers\\PaymentServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
